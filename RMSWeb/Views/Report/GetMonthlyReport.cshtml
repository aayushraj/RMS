@using RMS.Models
@model ReportModel


<script src="~/Scripts/custom.js"></script>
@{
    Layout = null;
}



<div class="card">
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-bordered table-hover tab-content" id="detailTable">

                <thead>
                    <tr class="danger">
                        <th>Paid Amount</th>
                        <th>PaymentDate</th>
                        <th>DueAmount</th>
                        <th>Advance</th>
                        <th>CreatedDate</th>

                    </tr>
                </thead>

                <tbody>

                    @foreach (var item in Model.list)
                    {
                        <tr class="info">
                            <td>@item.PaidAmount</td>
                            <td>@item.PaymentDate</td>
                            <td>@item.DueAmount</td>
                            <td>@item.Advance</td>
                            <td>@item.CreatedDate</td>
                        </tr>
                    }


                </tbody>

            </table>

        </div>
    </div>
</div>



